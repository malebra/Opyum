using System;

namespace Opyum.Playlist
{
    public interface IBaseAudioItem
    {
        /// <summary>
        /// Contains the information about the color in the program.
        /// </summary>
        string Color { get; set; }
        /// <summary>
        /// The duration of the song playing (in seconds, double).
        /// </summary>
        string EndCue { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string FiksniU_Terminu { get; set; }
        /// <summary>
        /// 
        /// </summary>
        string ID { get; set; }
        /// <summary>
        /// Aditional info.
        /// </summary>
        string ItemType { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string JediniU_Bloku { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string NaKanalu { get; set; }
        /// <summary>
        /// Statest the original (default) value of the "EndCue" field.
        /// </summary>
        string OriginalEndCue { get; set; }
        /// <summary>
        /// Statest the original (default) value of the "Pocetak" field.
        /// </summary>
        string OriginalPocetak { get; set; }
        /// <summary>
        /// Statest the original (default) value of the "StartCue" field.
        /// </summary>
        string OriginalStartCue { get; set; }
        /// <summary>
        /// Statest the original (default) value of the "Trajanje" field.
        /// </summary>
        string OriginalTrajanje { get; set; }
        /// <summary>
        /// The duration at whitch the singing begins (in seconds, double). 
        /// </summary>
        string Pocetak { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string PrviU_Bloku { get; set; }
        /// <summary>
        /// Reklama bool value (string).
        /// </summary>
        string Reklama { get; set; }
        /// <summary>
        /// Returns a bool for the ReklamaBool string variable.
        /// </summary>
        bool ReklamaBool { get; }
        /// <summary>
        /// Contains the value of the fade-in.
        /// </summary>
        string SoftIn { get; set; }
        /// <summary>
        /// Contains the value of the fade-out.
        /// </summary>
        string SoftOut { get; set; }
        /// <summary>
        /// The time in the song where the song begins to play (in seconds, double). Everything before that is skipped.
        /// </summary>
        string StartCue { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string StvarnoVrijemePocetka { get; set; }
        /// <summary>
        /// The type of the content. If it comes from FirePlay, the "Tip" variable is connected to the color when loading from inside the program.
        /// </summary>
        string Tip { get; set; }
        /// <summary>
        /// The point in the song at whitch the next song is supposed to be playing (in seconds, double).
        /// </summary>
        string Trajanje { get; set; }
        /// <summary>
        /// Contains the value of the volume.
        /// </summary>
        string Volume { get; set; }
        /// <summary>
        /// The time when the song is supposed to start playing.
        /// </summary>
        string Vrijeme { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string VrijemeMaxTermin { get; set; }
        /// <summary>
        /// ???
        /// </summary>
        string VrijemeMinTermin { get; set; }
        /// <summary>
        /// WaveIn bool value (string).
        /// </summary>
        string WaveIn { get; set; }
        /// <summary>
        /// Returns a bool for the WaveIn string variable.
        /// </summary>
        bool WaveInBool { get; }
        /// <summary>
        /// ???
        /// </summary>
        string ZadnjiU_Bloku { get; set; }


        event EventHandler InformationChange;
    }
}
